<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.rap.ui.entrypoint">
      <entrypoint
            class="org.codefaces.ui.entryPoints.CodeFacesEntryPoint"
            id="org.codefaces.ui.entrypoint.codefaces"
            parameter="codefaces">
      </entrypoint>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.codefaces.ui.perspectives.CodeFacesPerspectiveFactory"
            id="org.codefaces.ui.perspective.codefaces"
            name="Code Faces">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.codefaces.ui.category.codefaces"
            name="Code Faces">
      </category>
      <view
            allowMultiple="false"
            category="org.codefaces.ui.category.codefaces"
            class="org.codefaces.ui.views.ProjectExplorerViewPart"
            id="org.codefaces.ui.view.projectExplorer"
            name="Project Explorer"
            restorable="true">
      </view>
      <view
            allowMultiple="true"
            category="org.codefaces.ui.category.codefaces"
            class="org.codefaces.ui.views.CodeExplorerViewPart"
            id="org.codefaces.ui.view.codeExplorer"
            name="Code Explorer"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.rap.ui.branding">
      <branding
            defaultEntrypointId="org.codefaces.ui.entrypoint.codefaces"
            id="org.codefaces.ui.branding.codefaces"
            servletName="explore"
            title="Code Faces">
      </branding>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            name="Mail"
            id="maildemo.category">
      </category>
      <command
            name="Open Mailbox"
            description="Opens a mailbox"
            categoryId="maildemo.category"
            id="maildemo.open">
      </command>
      <command
            name="Open Message Dialog"
            description="Opens a message dialog"
            categoryId="maildemo.category"
            id="maildemo.openMessage">
      </command>
   </extension>
</plugin>
