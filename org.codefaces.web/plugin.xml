<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.equinox.http.registry.resources">
      <resource
            alias="/html"
            base-name="public">
      </resource>
   </extension>
   <extension
         point="org.eclipse.equinox.http.registry.servlets">
      <servlet
            alias="/"
            class="org.codefaces.web.UrlDispatcher"
            load-on-startup="true">
      </servlet>
   </extension>
   
   <extension
         point="org.eclipse.rap.ui.resources">
      <resource
            class="org.codefaces.ui.lib.js.JQueryResource">
      </resource>
      <resource
            class="org.codefaces.ui.lib.js.JQueryJsonpResource">
      </resource>
      <resource
            class="org.codefaces.ui.lib.js.Json2Resource">
      </resource>
   </extension>
</plugin>
