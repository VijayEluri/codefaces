<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.codefaces.core.models.RepoContainer"
            class="org.codefaces.core.RepoContainerAdapterFactory">
         <adapter
               type="java.util.Set">
         </adapter>
      </factory>
      <factory
            adaptableType="org.codefaces.core.models.RepoResource"
            class="org.codefaces.core.RepoResourceAdapterFactory">
         <adapter
               type="org.codefaces.core.models.Repo">
         </adapter>
      </factory>
      <factory
            adaptableType="org.codefaces.core.models.RepoFileLite"
            class="org.codefaces.core.RepoFileAdapterFactory">
         <adapter
               type="org.codefaces.core.models.RepoFile">
         </adapter>
      </factory>
   </extension>

</plugin>
